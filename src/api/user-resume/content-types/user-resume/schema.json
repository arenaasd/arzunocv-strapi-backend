{
  "kind": "collectionType",
  "collectionName": "user_resumes",
  "info": {
    "singularName": "user-resume",
    "pluralName": "user-resumes",
    "displayName": "User Resume",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "resumeId": {
      "type": "string"
    },
    "userEmail": {
      "type": "email"
    },
    "userName": {
      "type": "string"
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "jobTitle": {
      "type": "string"
    },
    "address": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "summary": {
      "type": "text"
    },
    "education": {
      "type": "component",
      "repeatable": true,
      "component": "education.education"
    },
    "experience": {
      "type": "component",
      "repeatable": true,
      "component": "experience.experience"
    },
    "skills": {
      "type": "component",
      "repeatable": true,
      "component": "skills.skills"
    },
    "themeColor": {
      "type": "string"
    },
    "Image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "projects": {
      "type": "component",
      "repeatable": true,
      "component": "projects.projects"
    },
    "selectedWorkType": {
      "type": "enumeration",
      "enum": [
        "projects",
        "experience"
      ]
    },
    "selectedExtraFields": {
      "type": "enumeration",
      "enum": [
        "hobbies",
        "languages",
        "certificates"
      ]
    },
    "certificates": {
      "type": "component",
      "repeatable": true,
      "component": "certificates.certificates"
    },
    "hobbies": {
      "type": "component",
      "repeatable": true,
      "component": "hobbies.hobbies"
    },
    "languages": {
      "type": "component",
      "repeatable": true,
      "component": "languages.languages"
    }
  }
}